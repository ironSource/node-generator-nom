{
  "name": "<%= moduleName %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "license": "<%= license %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },<% if (cli) { %>
  "bin": "cli.js",<% } %>
  "scripts": {
    "test": "<%= testCommand %>"
  },<% if (browserify) { %>
  "browserify": <%- browserify %>,<% } %><% if (browser) { %>
  "browser": <%- browser %>,<% } %>
  "dependencies": {},
  "devDependencies": {},<% if (keywords.length) { %>
  "keywords": <%- JSON.stringify(keywords) %>,<% } %><% if (repository) { %>
  "repository": <%- repository %>,<% } %><% if (homepage) { %>
  "homepage": <%- homepage %>,<% } %><% if (bugs) { %>
  "bugs": <%- bugs %>,<% } %>
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=2.0.0"
  }
}
